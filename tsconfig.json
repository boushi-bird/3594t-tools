{
  "compilerOptions": {
    "alwaysStrict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ESNext",
    "module": "es2015",
    "jsx": "react"
  },
  "exclude": ["node_modules"]
}
